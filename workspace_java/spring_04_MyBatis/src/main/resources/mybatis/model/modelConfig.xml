<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- content model은 configuration 태그 hover 했을 때 보이는
	 content type 순서대로 맞춰줘야 함 -->

<!-- tag(또는 node)의 순서에 주의 -->

	<!-- 전달인자로 null 줄 수 있음 -->
    <settings>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>
	
	<!-- java의 type란 거의 클래스 -->
	<typeAliases>
		<typeAlias type="kr.or.human4.dto.EmpDTO" alias="empDTO"/>
	</typeAliases>
</configuration>